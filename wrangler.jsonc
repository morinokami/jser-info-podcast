{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "jser-info-podcast",
	"main": "./dist/_worker.js/index.js",
	"compatibility_date": "2025-05-10",
	"compatibility_flags": ["nodejs_compat"],
	"assets": {
		"binding": "ASSETS",
		"directory": "./dist",
	},
	"observability": {
		"enabled": true,
	},
	"r2_buckets": [
		{
			"binding": "AUDIO",
			"bucket_name": "jser-info-podcast",
			"preview_bucket_name": "jser-info-podcast-dev",
		},
	],
}
